{
  "name": "whereabouts/whereabouts-php",
  "license": "MIT",
  "description": "PHP client for the Whereabouts API (https://whereabouts.blue)",
  "require": {
    "php": ">=5.6",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.4 || ^7.0",
    "phpstan/phpstan": "^0.11.5"
  },
  "autoload": {
    "psr-4": {
      "Whereabouts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Whereabouts\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
